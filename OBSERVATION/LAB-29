import heapq

def kClosest(points, k):
    max_heap = []
    for x, y in points:
        dist = -(x * x + y * y)
        if len(max_heap) < k:
            heapq.heappush(max_heap, (dist, x, y))
        else:
            heapq.heappushpop(max_heap, (dist, x, y))
    return [(x, y) for _, x, y in max_heap]
points = [[1, 3], [-2, 2], [2, -2]]
k = 2
print(kClosest(points, k))
